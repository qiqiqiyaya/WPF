<UserControl
    x:Class="Practice.Views.HomeView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Practice.Views"
    xmlns:lvc="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModels="clr-namespace:Practice.ViewModels"
    x:Name="Home"
    d:DataContext="{d:DesignInstance viewModels:HomeViewModel}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    Background="AntiqueWhite"
    mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="500" />
            <RowDefinition />
        </Grid.RowDefinitions>

        <lvc:CartesianChart Title="{Binding CpuTitle}" Series="{Binding CpuSeries}" />
        <TextBlock Grid.Row="1" Text="fdsafds" />
    </Grid>
</UserControl>
